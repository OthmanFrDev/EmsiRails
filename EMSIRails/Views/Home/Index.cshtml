@model IEnumerable<EMSIRails.Models.gare>
@{
    ViewBag.Title = "Home Page";
}

<div class="row m-0  peach-gradient color-block" style="position:relative;">
    <img src="~/Content/img/bghome.png" />
</div>
<div class="container" id="homerecherche">
    <div class="row justify-content-center m-auto">
        <!---->
        <div class="wrapper">
            <div id="formContent" style="opacity:1; position:absolute;top:10%;z-index:1;text-align:center;">
                <!-- Tabs Titles -->
                <!-- Icon -->
                <div class="fadeIn first">
                    <img src="~/Content/img/BookingBus (3).png" id="icon" alt="User Icon" />
                </div>

                <!-- Login Form -->
                <div class="container">
                    <form action="@Url.Action("Rechercher","Home")">
                        <div class="form-group m-2">
                            <label>Gare Départ</label>
                            <select class="form-control" name="Depart" required>
                                <option>--Gare Départ--</option>
                                @foreach (var item in Model)
                                {
                                    <option value="@item.idgare">@item.libelle</option>
                                }
                            </select>
                        </div>
                        <div class="form-group m-2">
                            <label>Gare Arrivée</label>
                            <select class="form-control" name="Arrive" required>
                                <option>--Gare Arrivée--</option>
                                @foreach (var item in Model)
                                {
                                    <option value="@item.idgare">@item.libelle</option>
                                }
                            </select>
                        </div>
                        <div class="form-group m-2">
                            <label>Date</label>
                            <input type="date" class="form-control" name="d"/>
                        </div>
                        <input type="submit" class="fadeIn fourth"  value="Rechercher" style="background-color: #F59A2E">
                    </form>
                </div>
                <!-- Remind Passowrd -->
            </div>
        </div>
        <!---->
    </div>
</div>
<div class="container mt-5 mb-5">
    <div class="row justify-content-md-center">
        <div class="col-12" style="text-align:center">
            <p style="font-size:1.5em;"><strong style="color:#F59A2E;">ExpressTrain.ma</strong> vous permet de chercher et acheter votre billet de Train en esp&#232;ces ou par carte bancaire.</p>
        </div>
    </div>
</div>
